  "license": "MIT",
  "dependencies": {
    "@vue/component-compiler-utils": "^3.0.2",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "bulma": "^0.8.0",
    "concurrently": "^5.0.0",
    "parcel": "^1.12.4",
    "pug": "^2.0.4",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.23.3",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.4",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10"
  },
  "scripts": {
    "parcel-build": "parcel build ./src/main.js --public-url .",
    "parcel-watch": "parcel watch ./src/main.js --public-url .",
    "pug": "pug index.pug --out dist",
    "serve": "python server.py",
    "start": "yarn run pug; concurrently -rk \"yarn run serve\" \"yarn run parcel-watch\""
